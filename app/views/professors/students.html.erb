<%= link_to "My Courses", professor_path %>
<%= link_to "All Courses", courses_path %>
<%= link_to "My Students", my_students_path %>
<%= form_tag logout_path, method: :delete do %>
    <%= submit_tag "Log Out" %>
<% end %>

<%# We can sort the students aphabetically %>
<% @professor.students.each do |stu| %>
    <li><a href="<%= student_path(stu) %>"><%= stu.full_name %></a></li>
<% end %>
